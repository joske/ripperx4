<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.8.2 -->
<interface>
  <!-- interface-name ripperx4.ui -->
  <requires lib="gtk" version="4.0"/>
  <object class="GtkApplicationWindow" id="window">
    <property name="default-height">600</property>
    <property name="default-width">400</property>
    <property name="icon-name">src/xpms/exit.xpm</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="vexpand">True</property>
        <child>
          <object class="GtkBox">
            <child>
              <object class="GtkImage">
                <property name="file">src/xpms/ripperX.xpm</property>
                <property name="hexpand">True</property>
                <property name="icon-name">ripperX</property>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <child>
                  <object class="GtkButton" id="scan_button"/>
                </child>
                <child>
                  <object class="GtkButton" id="cddb_button"/>
                </child>
                <child>
                  <object class="GtkButton" id="go_button"/>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="exit">
                <signal name="clicked" handler="exit_clicked"/>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkFrame">
            <property name="label">ripperX</property>
            <child>
              <object class="GtkBox">
                <property name="hexpand">True</property>
                <property name="orientation">vertical</property>
                <property name="vexpand">True</property>
                <child>
                  <object class="GtkScrolledWindow"/>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkStatusbar" id="statusbar">
            <property name="hexpand">True</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
